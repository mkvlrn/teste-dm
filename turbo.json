{
  "$schema": "node_modules/turbo/schema.json",
  "ui": "tui",
  "dangerouslyDisablePackageManagerCheck": true,
  "tasks": {
    "build": {
      "cache": true,
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "test": {
      "cache": false
    },
    "biome-check": {
      "cache": false
    },
    "biome-fix": {
      "cache": false
    },
    "typecheck": {
      "cache": false
    },
    "lint-staged": {
      "cache": false
    }
  }
}
