{
  "name": "@apps/backend",
  "private": true,
  "type": "module",
  "engines": {
    "node": "24.x"
  },
  "scripts": {
    "build": "vite build",
    "dev": "tsx --env-file-if-exists=.env --watch src/main.ts",
    "start": "node build/main.js",
    "test": "vitest",
    "test-e2e": "vitest --config ./e2e/vite.config.ts",
    "biome-check": "biome check --no-errors-on-unmatched",
    "biome-fix": "npm run biome-check --write",
    "typecheck": "tsc --noEmit",
    "reset-data": "tsx --env-file-if-exists=.env src/shared/utils/reset.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.0",
    "@nestjs/testing": "^11.1.6",
    "@repo/configs": "*",
    "@types/express": "^5.0.3",
    "@types/node": "^24.3.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^3.2.4",
    "lint-staged": "^16.1.5",
    "prisma": "^6.14.0",
    "rollup-plugin-node-externals": "^8.1.0",
    "supertest": "^7.1.4",
    "tsc-files": "^1.1.4",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vite": "^7.1.3",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.2.4",
    "vitest-mock-extended": "^3.1.0"
  },
  "dependencies": {
    "@faker-js/faker": "^9.9.0",
    "@fnando/cpf": "^1.0.2",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/platform-express": "^11.1.6",
    "@prisma/client": "^6.14.0",
    "@repo/error-codes": "*",
    "@repo/result": "*",
    "@repo/schemas": "*",
    "argon2": "^0.44.0",
    "better-auth": "^1.3.7",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.2",
    "zod": "^4.0.17"
  }
}
